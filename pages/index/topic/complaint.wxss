.head-title{
   padding:14rpx 30rpx;
   font-size:32rpx;
   text-align: left;
   color:#666;
}
.complaint-item{
   position: relative;
   padding:35rpx 30rpx;
   font-size: 34rpx;
   /* font-weight: bold; */
   background:#fff;
   border-bottom:1px solid #eee;
}
.complaint-item-name{
   font-family: '微软雅黑';
   color: #000;
}
.complaint-item-checked{
   position: absolute;
   right:30rpx;
   top:50%;
   margin-top:-15px;
}
.iconfont-right{
   position: absolute;
   right:30rpx;
   top:50%;
   margin-top:-22rpx;
   font-size:45rpx;
   color:#333;
}
.submit-btn{
   font-size: 36rpx;
   color:#fff;
   background:#d85940;
   margin:50rpx 30rpx 30rpx;
}

